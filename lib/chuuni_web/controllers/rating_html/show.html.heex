<.header>
  <%= @rating.author.name %>'s rating of <%= @rating.item_rated %>
  <:subtitle><%= @rating.updated_at %></:subtitle>
  <:actions>
    <.link href={~p"/ratings/#{@rating}/edit"}>
      <.button>Edit rating</.button>
    </.link>
  </:actions>
</.header>


<p class="text-2xl"><%= @rating.value %></p>

<.back navigate={~p"/ratings"}>Back to ratings</.back>
